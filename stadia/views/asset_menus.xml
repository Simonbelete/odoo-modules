<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <menuitem id="asset_menu" name="Asset Management" web_icon="stadia,static/Asset.png">
        <menuitem id="assets_menu" name="Assets"  action="action_asset" />
        <menuitem id="asset_types" name="Asset Types" action="action_asset_category" />
        <menuitem id="asset_reporting" name="Asset Reporting" action="action_report_asset" />
    </menuitem>
    <template id="asset_backend" name="Asset Report" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/stadia/static/src/js/asset_report.js"/>
        </xpath>
    </template>
</odoo>