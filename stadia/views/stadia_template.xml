<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="asset_backend" name="stadia assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/stadia/static/src/js/asset_report.js"/>
            <script type="text/javascript" src="/stadia/static/src/js/manpower_report.js"/>
        </xpath>
    </template>

    <template id="assets_common" name="stadia common assets" inherit_id="web.assets_common">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/ada_appraisal/static/libs/tailwindcss-v3.1.3.js" />
            <link rel="stylesheet" type="text/scss" href="/stadia/static/src/scss/input.scss" />
        </xpath>
    </template>
</odoo>