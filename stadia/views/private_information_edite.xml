        <record id="field_add_example_training_info" model="ir.ui.view">
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="priority" eval="14"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='personal_information']" position="inside">
                    <sheet>
                        <group string="Training Detail Information">
                            <field name="training_name_ids">
                                <tree editable="bottom" class="custom_class">
                                    <field name="type_of_training"/>
                                    <field name="name_of_institution_id"/>
                                    <field name="reward"/>
                                    <field name="document_for_training"/>
                                </tree>
                            </field>
                        </group>
                    </sheet>
                </xpath>
            </field>
        </record>
        <!--        EMPLOYEE CHILD INFORMATIONS -->
        <record id="field_add_example_child_info" model="ir.ui.view">
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="priority" eval="16"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='personal_information']" position="inside">
                    <sheet>
                        <group string="Child Information">
                            <field name="child_info_ids">
                                <tree editable="bottom" class="custom_class">
                                    <field name="child_full_name"/>
                                    <field name="child_gender" string="Gender"/>
                                    <field name="child_current"/>
                                    <field name="child_phone"/>
                                    <field name="document_for_brith_certificate"/>
                                </tree>
                            </field>
                        </group>
                    </sheet>
                </xpath>
            </field>
        </record>
        <!--        END OF EMPLOYEE INFORMATION-->

