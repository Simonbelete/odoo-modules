<odoo>
    <data>
        <record model="ir.ui.view" id="hr_employee_inherited_view_private_info">
            <field name="name">hr.employee.inherited.private_info</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='birthday']" position="replace">
                    <hr/>
                    <field name="birthday1"/>
                    <hr/>
                    <field name="age_com"/>
                    <hr/>
                    <field name="pension_number"/>
                    <hr/>
                    <field name="tin_number"/>
                    <hr/>
                </xpath>
                <xpath expr="//page[@name='public']/div/div/group[@name='managers']" position="replace">
                    <hr/>
                </xpath>
                <xpath expr="//page[@name='personal_information']/group/group[2]/field[1]" position="before">
                    <hr/>
                </xpath>
                <xpath expr="//page[@name='personal_information']/group/group[2]/field[1]" position="after">
                    <hr/>
                </xpath>
                <xpath expr="//page[@name='personal_information']/group/group[2]/field[2]" position="after">
                    <hr/>
                </xpath>
                <xpath expr="//page[@name='personal_information']/group/group[2]/field[3]" position="after">
                    <hr/>
                </xpath>
                <!--                <xpath expr="//page[@name='personal_information']/group/group[2]/field[4]" position="after">-->
                <!--                    <hr/>-->
                <!--                </xpath>-->
                <xpath expr="//page[@name='personal_information']/group/group[2]/field[@name='place_of_birth']"
                       position="after">
                    <hr/>
                </xpath>
                <xpath expr="//page[@name='personal_information']/group/group[2]/field[@name='country_of_birth']"
                       position="after">
                    <hr/>
                </xpath>
            </field>
        </record>
        <!--        SPOUSE EDITED-->
        <record model="ir.ui.view" id="hr_employee_inherited_view_spouse_info">
            <field name="name">hr.employee.inherited.spouse_info</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="priority" eval="9"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='spouse_birthdate']" position="replace">
                    <field name="current_address_f"
                           attrs="{'invisible': [('marital', 'not in', ['married', 'cohabitant'])]}"/>
                    <hr/>
                    <field name="spouse_mother_full_name"
                           attrs="{'invisible': [('marital', 'not in', ['married', 'cohabitant'])]}"/>
                    <hr/>
                    <field name="spouse_mother_phone"
                           attrs="{'invisible': [('marital', 'not in', ['married', 'cohabitant'])]}"/>
                    <hr/>
                    <field name="spouse_father_full_name"
                           attrs="{'invisible': [('marital', 'not in', ['married', 'cohabitant'])]}"/>
                    <hr/>
                    <field name="work" attrs="{'invisible': [('marital', 'not in', ['married', 'cohabitant'])]}"/>
                    <hr/>
                    <field name="phone_spouse"
                           attrs="{'invisible': [('marital', 'not in', ['married', 'cohabitant'])]}"/>
                    <hr/>
                    <field name="spouse_date_of_birth"
                           attrs="{'invisible': [('marital', 'not in', ['married', 'cohabitant'])]}"/>
                </xpath>
                <xpath expr="//field[@name='spouse_complete_name']" position="replace">
                    <field name="spouse_full_name"
                           attrs="{'invisible': [('marital', 'not in', ['married', 'cohabitant'])]}"/>
                </xpath>
            </field>
        </record>
        <!--        -->
        <record model="ir.ui.view" id="hr_employee_inherited_view_private_info_two">
            <field name="name">hr.employee.inherited.private_info_Two</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='address_home_id']" position="before">
                    <hr/>
                    <field name="woreda"/>
                    <hr/>
                    <field name="city"/>
                    <hr/>
                    <field name="sub_city"/>
                    <hr/>
                    <!--                    <field name="kebe"/>-->
                    <field name="house_number"/>
                    <hr/>
                    <field name="tell"/>
                </xpath>
                <xpath expr="//page[@name='personal_information']/group/group[1]/field[@name='address_home_id']"
                       position="after">
                    <hr/>
                </xpath>
                <xpath expr="//page[@name='personal_information']/group/group/field[@name='marital']"
                       position="after">
                    <hr/>
                </xpath>
                <xpath expr="//page[@name='personal_information']/group/group/field[@name='spouse_full_name']"
                       position="after">
                    <hr/>
                </xpath>
                <xpath expr="//page[@name='personal_information']/group/group[1]/field[@name='private_email']"
                       position="replace">
                    <field name="pr_email"/>
                    <hr/>
                </xpath>
                <xpath expr="//page[@name='personal_information']/group/group[1]/field[@name='phone']"
                       position="replace">
                    <field name="mobile_phone_two"/>
                    <hr/>
                </xpath>
            </field>
        </record>
        <!--        #family information-->
        <record model="ir.ui.view" id="hr_employee_inherited_view_family_info">
            <field name="name">hr.employee.inherited.private_info_Family_info</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='personal_information']/group/group[4]"
                       position="replace">
                    <group string="Family Information">
                        <hr/>
                        <field name="relative"/>
                        <hr/>
                        <field name="relativ_two"/>
                        <hr/>
                        <field name="m_phone"/>
                        <hr/>
                    </group>
                </xpath>
            </field>
        </record>
